@model IEnumerable<CoreTemplate.Models.Ogrenci>

@{
    ViewData["Title"] = "ListeKatilmayanlar";
}

<h1>Liste Katilmayanlar</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table table-hover" border="1">
    <thead>
        <tr>
            <th>
                Adı
            </th>
            <th>
                Soyadı
            </th>
            <th>
                Telefon
            </th>
            <th>
                E-posta
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.Ad
                </td>
                <td>
                    @item.Soyad
                </td>
                <td>
                    @item.Telefon
                </td>
                <td>
                    @item.Eposta
                </td>
            </tr>
        }
    </tbody>
</table>
